    "version": "0.0.1",
    "configurations": [
        {
            "name": "Triangle-Wars (Chrome)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--web-port",
                "9222",
                "-d",
                "chrome"
            ]
        },
        {
            "name": "Triangle-Wars (Edge)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--web-port",
                "9223",
                "-d",
                "edge"
            ]
        },
        {
            "name": "Triangle-Wars (Windows)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "windows"
            ]
        },
        {
            "name": "Triangle-Wars (Android)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "android"
            ]
        },
        {
            "name": "Triangle-Wars",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Triangle-Wars (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Triangle-Wars (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}
